<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Search Artist in Spotify</title>
    <link rel="stylesheet" href="assets/css/style.css">
</head>

<body>
    <a href="/">Home</a>
    <h1>Search Artist in Spotify</h1>
    <div id="searchArtists">
        <% artists.items.forEach(e => { %>
        <figure>
            <% e.images.forEach(se => { %>
            <% if (se.height == 640) { %>
            <img src="<%= se.url %> " alt="<%= e.name %> ">
            <% } %>
            <% }) %>
            <figcaption>
                <p><%= e.name %> </p>
                <a href='/details/artist/<%= e.id %> '>Details</a>
            </figcaption>
        </figure>
        <% }) %>
    </div>
</body>

</html>